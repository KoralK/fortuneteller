{
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py",
            "methods": [
                "GET",
                "POST",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET,POST,OPTIONS",
                "Access-Control-Allow-Headers": "*"
            }
        }
    ],
    "framework": null,
    "functions": {
        "api/index.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}